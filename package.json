{
  "name": "ilce-sekreterlik",
  "version": "1.0.0",
  "description": "İlçe Sekreterlik Uygulaması",
  "scripts": {
    "build": "cd sekreterlik-app/client && npm install && npm run build",
    "vercel-build": "npm run build",
    "dev:client": "cd sekreterlik-app/client && npm run dev",
    "dev:server": "cd sekreterlik-app/server && npm run dev",
    "install:all": "cd sekreterlik-app/client && npm install && cd ../server && npm install",
    "push": "bash scripts/auto-push.sh",
    "commit-and-push": "npm run commit-and-push:helper",
    "commit-and-push:helper": "git add -A && git commit -m \"Auto commit: $(date '+%Y-%m-%d %H:%M:%S')\" && bash scripts/auto-push.sh"
  },
  "dependencies": {
    "@sentry/node": "^10.22.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "firebase-admin": "^13.6.0",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^9.1.3",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^2.2.16",
    "sqlite3": "^5.1.7",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5"
  }
}