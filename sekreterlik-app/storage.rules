rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Tüm authenticated kullanıcılar için genel izin
    // Bu kural tüm dosyalar için geçerlidir
    match /{allPaths=**} {
      // Authenticated kullanıcılar okuyabilir ve yazabilir
      allow read, write: if request.auth != null;
    }
  }
}

